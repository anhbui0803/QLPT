{
    "version": 2,
    "builds": [
      {
        "src": "main.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      { "src": "/(.*)", "dest": "main.py" },
      { "src": "/static/(.*)", "headers": { "cache-control": "public, max-age=31536000" }, "dest": "/static/$1" }
    ]
  }